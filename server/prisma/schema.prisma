generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ranking {
  id       String   @id @default(uuid()) @db.Uuid
  name     String
  time     Int
  date     DateTime @default(now())
  scenario String?
}

model Characters {
  id     String @id @default(uuid()) @db.Uuid
  name   String @unique
  xrange Int[]
  yrange Int[]
}
